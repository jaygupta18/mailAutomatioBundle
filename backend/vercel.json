{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "FLASK_ENV": "production"
  },
  "functions": {
    "server.py": {
      "maxDuration": 30
    }
  }
}
